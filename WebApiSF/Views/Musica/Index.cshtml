@model System.Data.DataTable

@{
    ViewBag.Title = "Index";
}
<table class="table table-bordered table striped">
    <tr>
        <th>Banda</th>
        <th>Musica</th>
    </tr>
    @for (int i = 0; i < Model.Rows.Count;i++) //é como se fosse DataTable
    {
        <tr>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
        </tr>   
    }

</table>

<a href="@Url.Action("Create","Musica")">Adicionar Musica</a>
